{
    "name": "yiisoft/yii-bootstrap3",
    "description": "The Twitter Bootstrap 3 extension for the Yii framework",
    "keywords": ["yii", "bootstrap"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-bootstrap3/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-bootstrap3"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com",
            "homepage": "http://www.yiiframework.com/"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/"
        },
        {
            "name": "Antonio Ramirez",
            "email": "amigo.cobos@gmail.com"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/core": "^1.0",
        "yiisoft/yii-jquery": "^1.0",
        "npm-asset/bootstrap": "3.3.* | 3.2.* | 3.1.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\bootstrap\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
